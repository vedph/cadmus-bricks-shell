{
  "name": "cadmus-bricks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-col": "ng build @myrmidon/cadmus-cod-location",
    "build-phs": "ng build @myrmidon/cadmus-mat-physical-size",
    "build-achr": "ng build @myrmidon/cadmus-refs-asserted-chronotope",
    "build-aid": "ng build @myrmidon/cadmus-refs-asserted-ids",
    "build-ass": "ng build @myrmidon/cadmus-refs-assertion",
    "build-chr": "ng build @myrmidon/cadmus-refs-chronotope",
    "build-decc": "ng build @myrmidon/cadmus-refs-decorated-counts",
    "build-deci": "ng build @myrmidon/cadmus-refs-decorated-ids",
    "build-doc": "ng build @myrmidon/cadmus-refs-doc-references",
    "build-ext": "ng build @myrmidon/cadmus-refs-external-ids",
    "build-his": "ng build @myrmidon/cadmus-refs-historical-date",
    "build-lok": "ng build @myrmidon/cadmus-refs-lookup",
    "build-prp": "ng build @myrmidon/cadmus-refs-proper-name",
    "build-viaf": "ng build @myrmidon/cadmus-refs-viaf-lookup",
    "build-blk": "ng build @myrmidon/cadmus-text-block-view",
    "build-flg": "ng build @myrmidon/cadmus-ui-flags-picker",
    "build-nts": "ng build @myrmidon/cadmus-ui-note-set",
    "build-img": "ng build @myrmidon/cadmus-img-annotator && ng build @myrmidon/cadmus-img-gallery && ng build @myrmidon/cadmus-img-gallery-iiif",
    "build-sdimg": "ng build @myrmidon/cadmus-sdimg-annotator && ng build @myrmidon/cadmus-sdimg-gallery",
    "build-gal": "ng build @myrmidon/cadmus-img-gallery",
    "build-3if": "ng build @myrmidon/cadmus-img-gallery-iiif",
    "build-cab": "ng build @myrmidon/cadmus-ui-custom-action-bar",
    "build-lib": "npm run build-doc && npm run build-ass && npm run build-lok && npm run build-his && npm run build-cab && npm run build-col && npm run build-phs && npm run build-aid && npm run build-chr && npm run build-decc && npm run build-deci && npm run build-ext && npm run build-achr && npm run build-prp && npm run build-viaf && npm run build-blk && npm run build-flg && npm run build-img && npm run build-nts && npm run build-3if && npm run build-sdimg"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.1",
    "@angular/cdk": "^16.0.0",
    "@angular/common": "^16.0.1",
    "@angular/compiler": "^16.0.1",
    "@angular/core": "^16.0.1",
    "@angular/forms": "^16.0.1",
    "@angular/material": "^16.0.0",
    "@angular/platform-browser": "^16.0.1",
    "@angular/platform-browser-dynamic": "^16.0.1",
    "@angular/router": "^16.0.1",
    "@myrmidon/auth-jwt-admin": "^1.0.6",
    "@myrmidon/auth-jwt-login": "^1.0.2",
    "@myrmidon/cadmus-api": "^2.0.1",
    "@myrmidon/cadmus-core": "2.0.2",
    "@myrmidon/ng-mat-tools": "2.0.0",
    "@myrmidon/ng-tools": "2.0.0",
    "@ngneat/effects-ng": "^3.1.2",
    "@ngneat/elf": "^2.3.1",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-devtools": "^1.2.1",
    "@ngneat/elf-entities": "^4.4.4",
    "@ngneat/elf-pagination": "^1.1.0",
    "@ngneat/elf-requests": "^1.7.0",
    "@recogito/annotorious": "^2.7.10",
    "@recogito/annotorious-openseadragon": "^2.7.10",
    "@recogito/annotorious-selector-pack": "^0.5.3",
    "gravatar": "^1.8.2",
    "marked": "^5.0.1",
    "ngx-markdown": "^16.0.0",
    "openseadragon": "^4.0.0",
    "rangy": "^1.3.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.1",
    "@angular/cli": "^16.0.1",
    "@angular/compiler-cli": "^16.0.1",
    "@types/jasmine": "~4.3.1",
    "@types/marked": "^4.3.0",
    "@types/node": "^20.1.2",
    "@types/openseadragon": "^3.0.5",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^16.0.1",
    "typescript": "~5.0.4"
  }
}
